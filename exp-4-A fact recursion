# include <stdio.h>
 int main(){
 	int i,fact=1,num;
 	printf("enter the no:");
 	scanf("%d",&num);
 	for (i=1;i<=num;i++)
	 {
 		fact=fact*i;
	 }
    printf("factorial of %d is : %d",num,fact);
    return 0;
 }
 
 
OUTPUT
enter the no:4
factorial of 4 is : 24
--------------------------------
Process exited after 1.71 seconds with return value 0
